{
  "name": "express-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "hardhat": "npx hardhat node",
    "compile": "npx hardhat compile",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "dev:deploy": "yarn deploy:localhost && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "deploy:localhost": "npx hardhat run --network localhost scripts/deploy.js",
    "deploy:mumbai": "yarn compile && npx hardhat run --network mumbai scripts/deploy.js",
    "build": "yarn deploy:mumbai && tsc && tsc-alias ",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "postinstall": "$(yarn bin)/pm2 install typescript",
    "start:dev": "pm2 start ecosystem.config.js",
    "kill": "pm2 kill",
    "test": "jest",
    "monit": "pm2 monit"
  },
  "dependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@openzeppelin/contracts": "^4.6.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/mongoose-unique-validator": "^1.0.6",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.1.0",
    "firebase": "^9.6.11",
    "helmet": "^5.0.1",
    "ipfs-http-client": "50.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "mongoose-unique-validator": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "pm2": "^5.1.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.7",
    "jest": "^27.4.7",
    "ts-generator": "^0.1.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0",
    "typechain": "^8.0.0",
    "typescript": "^4.5.4"
  }
}
